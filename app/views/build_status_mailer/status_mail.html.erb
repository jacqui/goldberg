Project : <%= @project.name %><br>
Build Status : <%= @build.status %><br>
Build Page URL : <%= project_build_url(:project_name => @project.name, :build_number => @build.number) %>

<% unless @build.change_list.blank? %>
  <p>
    Diff:<br>

    <pre>
      <%= @build.change_list %>
    </pre>
  </p>
<% end %>

<% unless @build.build_log.blank? %>
  <p>
    Build log:<br>

    <pre>
      <%= @build.build_log %>
    </pre>
  </p>
<% end %>
